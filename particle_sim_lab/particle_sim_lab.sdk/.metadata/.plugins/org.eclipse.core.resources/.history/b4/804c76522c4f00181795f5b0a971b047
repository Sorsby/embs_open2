#include <stdio.h>
#include "xil_types.h"
#include "xil_cache.h"
#include "xparameters.h"
#include "zybo_vga/display_ctrl.h"
#include "vga.h"
#include "simulation.h"

int num_particles = 1000;
int num_attractors = 2;

struct Particle particles[num_particles];
struct Attractor attractors[num_attractors];

int main(void) {
    init_platform();
	setupVGA();

	while (1) {
		updateSimulation();
		updateFrame();
	}

    cleanup_platform();
	return 0;
}
