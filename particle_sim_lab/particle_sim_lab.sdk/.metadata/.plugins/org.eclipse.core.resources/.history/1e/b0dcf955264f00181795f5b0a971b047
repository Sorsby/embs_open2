#include <stdio.h>
#include "xil_types.h"
#include "xil_cache.h"
#include "xparameters.h"
#include "zybo_vga/display_ctrl.h"
#include "vga.h"
#include "simulation.h"

int num_particles = 1000;
int num_attractors = 2;

int main(void) {
    init_platform();
	setupVGA();

	struct particle *particles;
	struct attractor *attractors;

	while (1) {
		updateSimulation();
		updateFrame();
	}

    cleanup_platform();
	return 0;
}
